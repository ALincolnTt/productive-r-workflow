# Identify complete cases
penguins_clean <- dplyr::filter(data, complete.cases(data))
range_depth <- range(penguins_clean$bill_depth_mm)
range_length <- range(penguins_clean$bill_length_mm)
# Plot using function -- second exercise
p1 <- plot_depth_v_length_by_species(
data = penguins_clean,
selected_species = "Adelie",
group = "island",
color = "red",
xrange = range_length,
yrange = range_depth
)
p2 <- plot_depth_v_length_by_species(
data = penguins_clean,
selected_species = "Gentoo",
group = "island",
color = "blue",
xrange = range_length,
yrange = range_depth
)
p3 <- plot_depth_v_length_by_species(
data = penguins_clean,
selected_species = "Chinstrap",
group = "island",
color = "green",
xrange = range_length,
yrange = range_depth
)
plots <- list(p1, p2, p3)
patchwork::wrap_plots(plots, axes = 'collect_y', axis_titles = 'collect')
